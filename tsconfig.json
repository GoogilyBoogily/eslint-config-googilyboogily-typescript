{
	"compilerOptions": {
		"allowJs": false,
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"declaration": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"lib": ["es2020"],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noImplicitThis": false,
		"noLib": false,
		"noUnusedLocals": false,
		"outDir": "./dist",
		"removeComments": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2020",
		"types": ["node"]
	}
}
